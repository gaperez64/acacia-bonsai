   benchmark_files['syntcomp21']['100s-1000s'] +
   benchmark_files['syntcomp21']['1000s-inf'] } }

fs = import ('fs')

foreach folder, testsets : benchmark_files
  foreach set, files : testsets
    foreach file : files
      filename = 'ltl' / folder / file
      if not fs.exists (filename)
        error ('File ' + filename + ' not found.')
      endif
      foreach backend, opt : backends
        benchmark (backend + '/' + file,
                   check_real_exe,
                   args : [ '-p',  ## prefix every line with the name of the test
                            opt,  ## use acacia-bonsai.  -+: acaplus, -l: ltlsync
                            '-F', files (filename) ],
                   suite : [ 
                     backend,
                     backend + '/' + set,
                     backend + '/' + folder + '/' + set,
                     backend + '/' + folder,
                     'all/' + file,
                     'all',
                     'all' + '/' + set,
                     'all' + '/' + folder + '/' + set,
                     'all' + '/' + folder
                   ],
                   timeout: 10)
      endforeach
    endforeach
  endforeach
endforeach
