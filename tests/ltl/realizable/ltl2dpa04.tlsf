INFO {
  TITLE:       "LTL -> DPA  -  Example 4  (Source: Acacia+)"
  DESCRIPTION: "DPA A with 2 priorities: a word u is accepted by A if the minimal priority seen infinitely often is even"
  SEMANTICS:   Mealy
  TARGET:      Mealy
}

MAIN {

  INPUTS {
    a;
    b;
  }

  OUTPUTS {
    p0;
    p1;
  }

  INVARIANTS {
    p0 <-> !p1;
  }

  GUARANTEES {
    F (a && b) <-> G F p0;
  }

}
